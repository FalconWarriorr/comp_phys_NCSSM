    "from math import pi, atanh, sqrt\n",
    "\n",
    "R = 6.371E6\n",
    "a = 6378137\n",
    "b = 6356752.314245\n",
    "\n",
    "e = sqrt(1-b**2/a**2)\n",
    "\n",
    "print(\"Standard surface area: \\t\\t\" + str(round(4*pi*R**2)))\n",
    "print(\"Moar complex surface area:\\t\" + str(round(2*pi*a**2*(1+(1-e**2)/e*atanh(e)))))\n"
   ]
  }
